<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            stylesheets="@style.css"
            fx:controller="lab06.Controller"
            prefWidth="500" prefHeight="600">
    <VBox spacing="50" alignment="CENTER">

        <TextArea fx:id="input"/>
        <TextArea fx:id="output"/>
        <HBox spacing="20" alignment="CENTER">
            <Button text="Find MPSS" onAction="#mpss"/>
            <Button text="Test Running Times" onAction="#time"/>
            <Button text="Edit Distance" onAction="#findEditDist"/>
        </HBox>
    </VBox>
</AnchorPane>
